# TAC-HUB Environment Variables
# Copy this file to .env.local and fill in your values

# Application
NEXT_PUBLIC_APP_NAME=TAC-HUB
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app

# Database (if using external database)
# DATABASE_URL=your_database_url_here

# Authentication (if using external auth)
# NEXTAUTH_SECRET=your_secret_here
# NEXTAUTH_URL=https://your-domain.vercel.app

# API Keys (if using external APIs)
# WEATHER_API_KEY=your_weather_api_key
# MAPS_API_KEY=your_maps_api_key

# Analytics (if using external analytics)
# GOOGLE_ANALYTICS_ID=your_ga_id
# VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# Email (if using email services)
# EMAIL_SERVER_HOST=smtp.example.com
# EMAIL_SERVER_PORT=587
# EMAIL_SERVER_USER=your_email
# EMAIL_SERVER_PASSWORD=your_password
# EMAIL_FROM=noreply@your-domain.com

# Storage (if using external storage)
# CLOUDINARY_URL=your_cloudinary_url
# AWS_ACCESS_KEY_ID=your_aws_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret
# AWS_REGION=your_aws_region
# AWS_BUCKET_NAME=your_bucket_name

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_FORUM=true
NEXT_PUBLIC_ENABLE_MONITORING=true
NEXT_PUBLIC_ENABLE_ADMIN=true

# Development
NODE_ENV=development
